
<i-cell-group>
  <i-cell title="地区" bind:click="openRegion" is-link value="{{labels}}"></i-cell>
  <i-cell title="工种类型" bind:click="open"  is-link value="{{labels1}}"></i-cell>
</i-cell-group>
<i-cascader visible="{{visible}}" default-value="{{value}}" title="所在地区" mode="region" bind:change="onChange" bind:close="onRegionClose"/>
<i-cascader visible="{{visible1}}" default-value="{{value1}}" title="所属工种" options="{{options}}" bind:change="onChange1" bind:close="onClose"/>