{
  "name": "weapp",
  "version": "1.0.0",
  "description": "小程序 UI 组件库",
  "main": "app.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development gulp --gulpfile build/build.js",
    "docs": "vuepress dev docs",
    "build": "cross-env NODE_ENV=production gulp --gulpfile build/build.js",
    "clean": "cross-env NODE_ENV=production gulp clean --gulpfile build/build.js",
    "prepare": "npm run build",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "init:commit": "commitizen init cz-conventional-changelog --save --save-exact --force",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:msidolphin/weapp.git"
  },
  "author": "msidolphin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msidolphin/weapp/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/msidolphin/weapp",
  "gitHooks": {
    "commit-msg": "node git-hooks/commit-msg.js"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "del": "^4.1.0",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.3",
    "gulp-less": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "less": "^2.7.3",
    "viewcode": "^0.0.27",
    "vuepress": "^1.0.2",
    "yorkie": "^2.0.0"
  }
}
